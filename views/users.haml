!!! 5
%html
  %head
    %title Users
    %link(rel="stylesheet" href="css/bootstrap.min.css")
    %link(rel="stylesheet" href="css/style.css")
    %script(src="js/libs/modernizr-2.0.6.min.js")

  %body
    #container.container
      %header
        %h1 Users

      #main
        - for user in users
          %div
            %a(href="?user_id=#{user.id}")= user.user

  #legend

  %script(src="js/libs/jquery-1.7.2.min.js")
  %script(src="js/libs/bootstrap.min.js")
